            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.jun</groupId>
            <artifactId>CATHomeDataBase</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.jun</groupId>
            <artifactId>CATHomeUtils</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
    </dependencies>
</project>